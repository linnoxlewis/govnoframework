{
    "name": "linnoxlewis/my-very-bad-framework",
    "type": "project",
    "description": "Custom Framework.",
    "keywords": [
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "7.*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },

    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "minimum-stability": "dev"
}
